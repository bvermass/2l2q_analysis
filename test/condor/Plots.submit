executable = execPlots
arguments = $(inputfile) $(max_entries) 1 0

should_transfer_files = YES
log = test/condor/log/Plot_$(ProcId).log
output = test/condor/log/Plot_$(ProcId).out
error = test/condor/log/Plot_$(ProcId).err

request_cpus = 1
request_disk = 1000
request_memory = 2000

queue inputfile,max_entries from test/condor/MiniAnalyzerJobList.txt
